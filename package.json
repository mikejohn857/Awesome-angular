    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.5.4",
    "angular-in-memory-web-api": "^0.6.1",
    "codelyzer": "^4.4.2",
    "concurrently": "^3.6.1",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.3",
    "fs-extra": "^7.0.0",
